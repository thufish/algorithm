//

#include "stdafx.h"
#include <vector>
#include <list>
#include <string>
#include <iterator>
using namespace std;

int _tmain(int argc, _TCHAR* argv[])
{
	string str;
	cin >> str;
	string str_result;

	for (int i=0;i<=str.size()-1;i++)
	{
		int strat_pos=i;
		char max_so_far=0;
		for (int j=str.size()-1;j>=i;j--)
		{
			if (str[j]>=max_so_far)
			{
				max_so_far=str[j];
				strat_pos=j;
			}
		}
		i=strat_pos;
		str_result+=max_so_far;
	}
	cout << str_result;
	system("pause");
	return 0;
}


